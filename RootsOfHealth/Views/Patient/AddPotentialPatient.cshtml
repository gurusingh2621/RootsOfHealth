        function validateDOB() {
            var dob = $('#txtDOB').val();

            var pattern = /^([0-9]{2})\/([0-9]{2})\/([0-9]{4})$/;
            if (!pattern.test(dob)) {
                toastr.error("Date is not in correct format")
                $('#txtDOB').addClass('invalid-field');
                return false;
            }
            else {
                var date = new Date(dob);
                var year = date.getFullYear();
                var currentYear = new Date().getFullYear() + 1

                if (year > 1900 && year <= currentYear) {
                    return true;
                }

                toastr.error("Birth year should be in between 1900 and " + currentYear + '.');
                $('#txtDOB').addClass('invalid-field');
                return false
            }
        }

        function AddDate() {

            var d = new Date();
            var month = d.getMonth() + 1;
            var day = d.getDate();
            var time = d.getHours() + ":" + d.getMinutes() + ":" + d.getSeconds();
            CreatedDate = d.getFullYear() + '/' +
                (month < 10 ? '0' : '') + month + '/' +
                (day < 10 ? '0' : '') + day + "  " + time;

            ShortDate = d.getFullYear() + '/' +
                (month < 10 ? '0' : '') + month + '/' +
                (day < 10 ? '0' : '') + day;

            ModifiedDate = d.getFullYear() + '/' +
                (month < 10 ? '0' : '') + month + '/' +
                (day < 10 ? '0' : '') + day + "  " + time;
        }

