                                                       id="InvolvedRadioYes" onclick="Radio()" name="CommunityGroup"
                                                       value="true">
                                                <label class="custom-control-label"
                                                       for="InvolvedRadioYes">Yes</label>
                                            </div>

                                        </div>
                                        <div class="two-columns">
                                            <div class="custom-control custom-radio d-inline-block mr-3">
                                                <label class="label-fields">@Html.Raw(Model.IsSomeoneYouCanCall == true ? "Yes" : "No")</label>
