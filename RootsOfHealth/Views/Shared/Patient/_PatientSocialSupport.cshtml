@model RootsOfHealth.Models.PatientSocialSupportBO

<div class="tab-pane fade" id="SocialSupports" role="tabpanel" aria-labelledby="SocialSupports-tab">
    <div class="inner-content">
        <div class="paitent-detail">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="paitent-detail-left">
                        <div class="detail-heading d-flex">
                            <h2>
                                Client Social Supports and
                                Safety
                            </h2>
                            <div class="detail-button ml-auto">

                                <a href="/Patient/Add?PatientID=@ViewBag.PatientId"
                                   class="btn btn-success label-fields">
                                    <i class="far fa-edit"></i>
                                    Edit
                                </a>
                                <a tabindex="1" href="javascript:{}" onclick="return SocialSupportsSave()" id="SocialSupportsSave" class="btn btn-success">
                                    <i class="far fa-save"></i>
                                    Save
                                </a>

                            </div>

                        </div>
                        <div class="inner-main-content scroll">
                            <div class="top-image-row d-flex">
                                <div class="top-image-left">


                                    <div class="form-row-first label-fields">
                                        <div class="top-form-row d-flex height-auto">
                                            <div class="two-columns">

                                                <label class="label-fields">
                                                    Score
                                                </label>
                                            </div>
                                            <div class="two-columns">

                                                <label class="label-fields">
                                                    Status
                                                </label>
                                            </div>

                                        </div>
                                        <div class="bootom-form-row d-flex">
                                            <div class="two-columns">

                                                <label id="txtsocialscore" class="label-fields"></label>

                                            </div>

                                            <div class="two-columns">

                                                <label id="txtsocialstatus" class="label-fields"></label>

                                            </div>
                                        </div>
                                    </div>





                                </div>
                            </div>
                            <form class="detail-patient-form" id="frmSocialSupports">
                                <div class="form-row-first">
                                    <div class="top-form-row d-flex height-auto">
                                        <div class="one-columns">
                                            <label class="Add">
                                                Is there someone now that you can depend on if you
                                                ever needed help to do a task, like getting a ride
                                                somewhere, or help with shopping or cooking a
                                                meal?
                                            </label>
                                            <label class="label-fields">
                                                Is there someone now that you can depend on if you
                                                ever needed help to do a task, like getting a ride
                                                somewhere, or help with shopping or cooking a
                                                meal?
                                            </label>
                                        </div>
                                    </div>
                                    <div class="bootom-form-row d-flex">
                                        <div class="two-columns">
                                            <div class="custom-control custom-radio d-inline-block mr-3">
                                                <label class="label-fields" id="lblYouCanDependOn">@Model.IsSomeoneYouCanDependOn</label>
                                                <input tabindex="1" type="radio" class="custom-control-input"
                                                       id="DependRadioNo" onclick="Radio()" name="DependOnRadio" value="No">
                                                <label class="custom-control-label"
                                                       for="DependRadioNo">No</label>
                                            </div>
                                            <div class="custom-control custom-radio d-inline-block mr-3">
                                                <input tabindex="1" type="radio" class="custom-control-input"
                                                       id="DependRadioYes" onclick="Radio()" name="DependOnRadio"
                                                       value="Yes">
                                                <label class="custom-control-label"
                                                       for="DependRadioYes">Yes</label>
                                            </div>

                                            <div class="custom-control custom-radio d-inline-block">
                                                <input tabindex="1" type="radio" class="custom-control-input"
                                                       id="DependRadioknow" onclick="Radio()" name="DependOnRadio"
                                                       value="Don’t Know">
                                                <label class="custom-control-label"
                                                       for="DependRadioknow">Don’t Know</label>
                                            </div>
                                        </div>


                                    </div>
                                </div>
                                <div class="form-row-first">
                                    <div class="top-form-row d-flex height-auto">
                                        <div class="two-columns">
                                            <label class="Add">
                                                Are you currently involved with any church, community
                                                group, club, etc.?
                                            </label>
                                            <label class="label-fields">
                                                Are you currently involved with any church, community
                                                group, club, etc.?
                                            </label>
                                        </div>
                                        <div class="two-columns">
                                            <label class="Add">
                                                Is there someone you can call when you’re feeling
                                                down or stressed?
                                            </label>
                                            <label class="label-fields">
                                                Is there someone you can call when you’re feeling
                                                down or stressed?
                                            </label>
                                        </div>
                                    </div>
