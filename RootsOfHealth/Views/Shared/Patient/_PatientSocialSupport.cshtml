                                            @Html.DropDownListFor(m => m.PlaceToStay, new SelectList(Model.ddlPlaceToStay, "Value", "Text", Model.PlaceToStay), new { @id = "ddPlaceToStay", @class = "form-control", @Name = "PlaceToStay", @tabindex = "1" })



